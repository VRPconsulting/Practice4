<aura:component description="mapEntry">
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:attribute name="map" type="map"/>
    <aura:attribute name="key" type="string"/>
    <aura:attribute name="value" type="List"/>

    <p>{!v.key} --- {!v.value}</p>
</aura:component>
